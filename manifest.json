{
  "manifest_version": 3,
  "name": "StaySafe - Gambling Site Blocker",
  "version": "1.0",
  "description": "Blocks gambling and betting websites to protect users.",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
